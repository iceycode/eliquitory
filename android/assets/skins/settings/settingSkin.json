{
  com.badlogic.gdx.graphics.g2d.BitmapFont: {
    default-font: { file: fonts/roboto-slap12.fnt },
    light-font: {file: fonts/roboto-slap-light12.fnt},
    medium-font: { file: fonts/roboto-reg14.fnt }
  },
  com.badlogic.gdx.graphics.Color: {
    green: { a: 1, b: 0, g: 1, r: 0 },
    white: { a: 1, b: 1, g: 1, r: 1 },
    red: { a: 1, b: 0, g: 0, r: 1 },
    black: {a: 1, b: 0, g: 0, r: 0},
    grey: { a: 1, b: .5, g: .5, r: .5}, //divide by 255 to get the float value
  },
  com.badlogic.gdx.scenes.scene2d.ui.Button$ButtonStyle: {
    default: { up: back-up, down: back-down}
  },
  com.badlogic.gdx.scenes.scene2d.ui.TextField$TextFieldStyle: {
    default: {selection: selection, background: titleBackground, font: default-font, fontColor: black, cursor: cursor,
      messageFont: light-font, messageFontColor: grey}
  },
  com.badlogic.gdx.scenes.scene2d.ui.TextButton$TextButtonStyle: {
    default: { up: buttonUp, down: buttonDown, font: default-font, fontColor: black},
  },
    com.badlogic.gdx.scenes.scene2d.ui.Label$LabelStyle: {
    default: { font: default-font, fontColor: black },
    title: {font: medium-font, fontColor: black, background: titleBackground }
  },
  com.badlogic.gdx.scenes.scene2d.ui.Slider$SliderStyle: {
    default-horizontal: { background: slider, knob: sliderKnob } //need to put -horizontal or -vertical identifier
  }
}